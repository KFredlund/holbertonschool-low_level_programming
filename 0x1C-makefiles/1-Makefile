# -*- MakeFile -*-

#target: dependencies
#	action
SRC = main.c holberton.c

CC = /usr/local/bin/gcc

all: holberton main.o holberton.o clean
.PHONY: all

holberton: main.o holberton.o
	gcc main.o holberton.o -o holberton

main.o: main.c
	gcc -c main.c

holberton.o: holberton.c
	gcc -c holberton.c

clean:
	rm -f main.o holberton.o
