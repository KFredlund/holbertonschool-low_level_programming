# -*- MakeFile -*-

#target: dependencies
#	action
TARGET = holberton
SRC = main.c holberton.c

CC = gcc -std=c99 -c

all: holberton main.o holberton.o clean
.PHONY: all

$(TARGET): main.o holberton.o
	gcc main.o holberton.o -o holberton

main.o: main.c
	$(CC) -c main.c

holberton.o: holberton.c
	$(CC) -c holberton.c

clean:
	rm -f main.o holberton.o
