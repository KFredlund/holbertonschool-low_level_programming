# -*- MakeFile -*-

#target: dependencies
#	action
SRC = main.c holberton.c

CC = /usr/local/bin/gcc

all: holberton main.o holberton.o
.PHONY: all

holberton: main.o holberton.o
	main.o holberton.o -o holberton

main.o: main.c
	gcc -c main.c

holberton.o: holberton.c
	gcc  -c holberton.c

clean: rm -f holberton main.o holberton.o
